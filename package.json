{
  "name": "threeify-glsl-transpiler",
  "version": "2.0.1",
  "description": "A glsl to JS module transpiler.",
  "keywords": [
    "glsl",
    "compiler",
    "threeify",
    "transpiler",
    "shaders"
  ],
  "author": "threeify authors",
  "license": "ISC",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/threeify/threeify-glsl-transpiler"
  },
  "bin": {
    "tgt": "./src/main.ts"
  },
  "main": "dist/lib",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm tsc",
    "start": "npx tsc && node ./dist/index.js",
    "lint": "eslint \"src/**\"",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.1.1",
    "@types/watch": "^1.0.3",
    "commander": "^9.4.1",
    "glob": "^8.0.3",
    "make-dir": "^3.1.0",
    "typescript": "^4.8.4",
    "watch": "^0.13.0"
  }
}
